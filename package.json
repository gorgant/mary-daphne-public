{
  "name": "mary-daphne",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:client-app:sandbox": "ng build -c=sandbox",
    "build:client-app:prod": "ng build --prod",
    "build:server-app:sandbox": "ng run mary-daphne:server:sandbox",
    "build:server-app:prod": "ng run mary-daphne:server:production",
    "build-all:sandbox": "npm-run-all build:client-app:sandbox build:server-app:sandbox",
    "build-all:prod": "npm-run-all build:client-app:prod build:server-app:prod",
    "prerender": "ts-node -P ./server.tsconfig.json ./prerender.ts",
    "start:express-server": "ts-node -P ./server.tsconfig.json ./server.ts",
    "move-index": "mv ./dist/mary-daphne/index.html ./functions/lib/app-bundle/index-server.html",
    "deploy": "firebase deploy",
    "build-and-deploy:sandbox": "npm-run-all build-all:sandbox move-index deploy",
    "build-and-deploy:prod": "npm-run-all build-all:prod move-index deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.1",
    "@angular/cdk": "~8.0.2",
    "@angular/common": "~8.1.1",
    "@angular/compiler": "~8.1.1",
    "@angular/core": "~8.1.1",
    "@angular/fire": "^5.2.1",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~8.1.1",
    "@angular/material": "^8.0.2",
    "@angular/material-moment-adapter": "^8.0.2",
    "@angular/platform-browser": "~8.1.1",
    "@angular/platform-browser-dynamic": "~8.1.1",
    "@angular/platform-server": "~8.1.1",
    "@angular/router": "~8.1.1",
    "@ngrx/effects": "^8.4.0",
    "@ngrx/entity": "^8.4.0",
    "@ngrx/router-store": "^8.4.0",
    "@ngrx/store": "^8.4.0",
    "@ngrx/store-devtools": "^8.4.0",
    "@nguniversal/express-engine": "^9.0.0-next.6",
    "@nguniversal/module-map-ngfactory-loader": "^9.0.0-next.6",
    "@types/stripe-v3": "^3.1.4",
    "compression": "^1.7.4",
    "firebase": "^6.6.2",
    "firebase-admin": "^8.7.0",
    "hammerjs": "^2.0.8",
    "moment": "^2.24.0",
    "ng-lazyload-image": "^7.0.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
